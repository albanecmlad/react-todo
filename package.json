{
  "name": "react-todo",
  "version": "0.0.0",
  "description": "",
  "repository": "bethazari/react-todo",
  "author": "bethazari",
  "license": "MIT",
  "keywords": [],
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": ["react", "import", "jsx-a11y"]
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "chai-immutable": "^1.5.4",
    "classnames": "^2.1.5",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "immutable": "^3.8.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "redux": "^3.5.2",
    "sinon": "^2.0.0-pre",
    "todomvc-app-css": "^2.0.0",
    "todomvc-common": "^1.0.1",
    "webpack": "1.13.x"
  },
  "scripts": {
    "lint": "eslint src/js tests",
    "test": "mocha --compilers js:babel-register --reporter spec tests",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch tests",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha tests",
    "build": "webpack --colors --progress",
    "start": ""
  }
}
