{
  "name": "react-mvc-babel",
  "version": "0.0.0",
  "description": "",
  "repository": "albanecmlad/react-mvc-babel",
  "author": "bethazari",
  "license": "MIT",
  "keywords": [],
  "main": "lib/index.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "7.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.3.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sinon": "^2.0.0-pre",
    "todomvc-app-css": "^2.0.0",
    "todomvc-common": "^1.0.1",
    "webpack": "1.13.x"
  },
  "scripts": {
    "lint": "eslint src/js tests",
    "test": "mocha --compilers js:babel-register tests",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch tests",
